    "@ipld/dag-ucan": "^3.4.0",
    "@ucanto/client": "^9.0.0",
    "@ucanto/core": "^9.0.0",
    "@ucanto/interface": "^9.0.0",
    "@ucanto/transport": "^9.0.0",
    "@w3ui/react": "2.3.0",
    "@web3-storage/access": "^19.0.0",
    "@web3-storage/capabilities": "^17.1.0",
    "@web3-storage/content-claims": "^3.2.1",
