    "nanoid-dictionary": "^4.3.0",
    "next": "^13.5.4",
    "nft.storage": "^7.1.1",
    "p-retry": "^6.2.0",
    "react": "latest",
    "react-dom": "latest",
    "react-hook-form": "^7.51.3",
    "react-hot-toast": "^2.4.1",
    "swr": "^2.2.4",
